<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="checkout_cart_product_add_after">
        <observer name="frontend_cartAdd" instance="ViaAds\Integration\Observer\Frontend\AddCart" />
    </event>
	<event name="sales_quote_remove_item">
        <observer name="frontend_cartRemove" instance="ViaAds\Integration\Observer\Frontend\RemoveCart" />
    </event>
	<event name="wishlist_add_product">
        <observer name="frontend_wishlistAdd" instance="ViaAds\Integration\Observer\Frontend\AddWishlist" />
    </event>
	<event name="controller_action_predispatch_wishlist_index_remove">
        <observer name="frontend_wishlistRemove" instance="ViaAds\Integration\Observer\Frontend\RemoveWishlist" />
    </event>
	<event name="catalog_controller_product_view">
        <observer name="frontend_pageView" instance="ViaAds\Integration\Observer\Frontend\PageView" />
    </event>
</config>